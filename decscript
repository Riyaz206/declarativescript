pipeline{
    agent any
         stages{
            stage ('check validate'){
               steps{
                        echo 'iam validating the code'
                        }
                                                  }
            stage ('Testing'){
               steps{
                        echo 'iam testing the code'
                        }
                                                }
             stage ('install'){
                steps{
                         echo 'iam installing the code'
                          }
                                                 }
              stage ('deploy'){
                 steps{
                          echo 'iam deploying the code'
                          }
                                                 }
            }
}
